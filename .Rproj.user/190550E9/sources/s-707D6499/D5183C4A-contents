```{r Conservatism Henningham}
###############
# Henningham #
#############
#
# Direction of means
# Conservative are HIGH, Liberals are LOW #-----------------------------------------
#
# Strongly agree (1) - Strongly disagree (9)
# -------------------------------------------
# Death penalty* 
# Multiculturalism 
# Stiffer jail terms* 
# Voluntary euthanasia 
# Bible truth* 
# Gay rights
# Premarital virginity* 
# Muslim immigration
# Church authority*
# Legalized abortion 
# Legalized prostitution 
# 1, 3, 5, 7, 9
# -----------------------
# Henn 97
# Government Welfare
# Unemployment Benefits
# Worker's Unions
# Wealth Tax
# Minimum Wage 
#
#
dataFull$CS5_Henn_1_REV  <-  sapply(dataFull$CS5_Henn_1,  function(x) 10 - x)
dataFull$CS5_Henn_3_REV  <-  sapply(dataFull$CS5_Henn_3,  function(x) 10 - x)
dataFull$CS5_Henn_5_REV  <-  sapply(dataFull$CS5_Henn_5,  function(x) 10 - x)
dataFull$CS5_Henn_7_REV  <-  sapply(dataFull$CS5_Henn_7,  function(x) 10 - x)
dataFull$CS5_Henn_9_REV <-  sapply(dataFull$CS5_Henn_9,  function(x) 10 - x)
#
# Overall Score Henninghan 1996 (Social Conservatism)
# ---------------------------------------------------
#dataFull$CS5_Henn_1996 <- dataFull$CS5_Henn_1 + dataFull$CS5_Henn_2_REV + dataFull$CS5_Henn_3 + dataFull$CS5_Henn_4_REV + dataFull$CS5_Henn_5 + dataFull$CS5_Henn_6_REV + dataFull$CS5_Henn_7 + dataFull$CS5_Henn_8_REV + dataFull$CS5_Henn_9 + dataFull$CS5_Henn_10_REV + dataFull$CS5_Henn_11_REV
#
dataFull$CS5_Henn_1996 <- rowMeans(dataFull[,c("CS5_Henn_1_REV", "CS5_Henn_2", "CS5_Henn_3_REV", "CS5_Henn_4", "CS5_Henn_5_REV", "CS5_Henn_6", "CS5_Henn_7_REV", "CS5_Henn_8", "CS5_Henn_9_REV", "CS5_Henn_10", "CS5_Henn_11")], na.rm=TRUE)
#
# Overall Score Henninghan 1997 (Economic Conservatism)
# -----------------------------------------------------
#dataFull$CS5_Henn_1997 <- dataFull$CS5_Henn_12_REV + dataFull$CS5_Henn_13_REV + dataFull$CS5_Henn_14_REV + dataFull$CS5_Henn_15_REV + dataFull$CS5_Henn_16_REV  
#
dataFull$CS5_Henn_1997 <- rowMeans(dataFull[,c("CS5_Henn_12", "CS5_Henn_13", "CS5_Henn_14", "CS5_Henn_15", "CS5_Henn_16")], na.rm=TRUE)
#
# Overall Score Henninghan All Items
# -----------------------------------
#dataFull$CS5_Henn_All <- dataFull$CS5_Henn_1996 + dataFull$CS5_Henn_1997

dataFull$CS5_Henn_All <- rowMeans(dataFull[,c("CS5_Henn_1_REV", "CS5_Henn_2", "CS5_Henn_3_REV", "CS5_Henn_4", "CS5_Henn_5_REV", "CS5_Henn_6", "CS5_Henn_7_REV", "CS5_Henn_8", "CS5_Henn_9_REV", "CS5_Henn_10", "CS5_Henn_11", "CS5_Henn_12", "CS5_Henn_13", "CS5_Henn_14", "CS5_Henn_15", "CS5_Henn_16")], na.rm=TRUE)
#
#
#doBy::summaryBy(CS5_Henn_1996 ~ Scr_PIDF, data=data.76, FUN=c(mean))
#doBy::summaryBy(CS5_Henn_1997 ~ Scr_PIDF, data=data.76, FUN=c(mean))
#doBy::summaryBy(CS5_Henn_All ~ Scr_PIDF, data=data.76, FUN=c(mean))
#
#
dataFull$HC <- dataFull$CS5_Henn_All
dataFull$HC.SC <- rowMeans(dataFull[c("CS5_Henn_1_REV", "CS5_Henn_2", "CS5_Henn_3_REV", "CS5_Henn_4", "CS5_Henn_5_REV", "CS5_Henn_6", "CS5_Henn_7_REV", "CS5_Henn_8", "CS5_Henn_9_REV", "CS5_Henn_10", "CS5_Henn_11")], na.rm=TRUE)
dataFull$HC.EC <- rowMeans(dataFull[c("CS5_Henn_12", "CS5_Henn_13", "CS5_Henn_14", "CS5_Henn_15", "CS5_Henn_16")], na.rm=TRUE)
