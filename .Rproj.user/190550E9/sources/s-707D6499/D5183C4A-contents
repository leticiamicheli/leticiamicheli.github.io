#fun <- function(x){c(m=mean(x),# v=var(x),n=length(x))}

#View(doBy::summaryBy(cbind(JS_Victim, JS_Observer, JS_Beneficiary, JS_Perpetrator) ~ PS_CandF, FUN=mean, data=df))

