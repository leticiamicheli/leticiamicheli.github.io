# x <- cor(df); corrplot::corrplot(x, type="upper", order="hclust")
#
#data <- data.76
#psych::describeBy(data$RWA_Conventionalism, group = data$Scr_PIDF)
#psych::describeBy(data$RWA_Submission, group = data$Scr_PIDF)
#psych::describeBy(data$RWA_Agression, group = data$Scr_PIDF)


#RWA <- data[,c("RWA_1", "RWA_2", "RWA_3_REV", "RWA_4_REV", "RWA_5", "RWA_6", "RWA_7_REV", "RWA_8_REV", "RWA_9", "RWA_10", "RWA_11_REV", "RWA_12_REV")]

