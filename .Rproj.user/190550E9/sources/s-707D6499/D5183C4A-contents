dataFull$CSJ_Econ_9_REV  <- sapply(dataFull$CSJ_Econ_9,  function(x) 10 - x)
dataFull$CSJ_Econ_11_REV <- sapply(dataFull$CSJ_Econ_11,  function(x) 10 - x)
dataFull$CSJ_Econ_12_REV <- sapply(dataFull$CSJ_Econ_12,  function(x) 10 - x)
dataFull$CSJ_Econ_14_REV <- sapply(dataFull$CSJ_Econ_14,  function(x) 10 - x)
dataFull$CSJ_Econ_16_REV <- sapply(dataFull$CSJ_Econ_16,  function(x) 10 - x)
#
#
#dataFull$SJ_Eco  <-  
 # dataFull$CSJ_Econ_1_REV + dataFull$CSJ_Econ_2_REV + dataFull$CSJ_Econ_3_REV + 
 #dataFull$CSJ_Econ_4 +
  #dataFull$CSJ_Econ_5_REV +
