```{r Quotas for all data}
#
#######################
# Quotas - Screening #
#####################
Quotas <- 
  rbind(
    rbind(
      as.data.frame(cbind(
        Count=table(dataFull$AgeF),
        Percentage=round(prop.table(table(dataFull$AgeF))*100,1))),
        Total_Age=colSums(
          as.data.frame(cbind(
            Count=table(dataFull$AgeF),
            Percentage=round(prop.table(table(dataFull$AgeF))*100,1))))),
    
    rbind(as.data.frame(cbind(
      Count=table(dataFull$SexF),
      Percentage=round(prop.table(table(dataFull$SexF))*100,1))),
      Total_Gender=colSums(
        as.data.frame(cbind(
          Count=table(dataFull$SexF),
          Percentage=round(prop.table(table(dataFull$SexF))*100,1))))),
    
    rbind(as.data.frame(cbind(
      Count=table(dataFull$IncF),
      Percentage=round(prop.table(table(dataFull$IncF))*100,1))),
      Total_Income=colSums(
        as.data.frame(cbind(
          Count=table(dataFull$IncF),
          Percentage=round(prop.table(table(dataFull$IncF))*100,1))))),
  
    rbind(as.data.frame(cbind(
      Count=table(dataFull$EduF),
      Percentage=round(prop.table(table(dataFull$EduF))*100,1))),
      Total_Education=colSums(
        as.data.frame(cbind(
          Count=table(dataFull$EduF),
          Percentage=round(prop.table(table(dataFull$EduF))*100,0))))),
  
    rbind(as.data.frame(cbind(
      Count=table(dataFull$P.ID),
      Percentage=round(prop.table(table(dataFull$P.ID))*100,1))),
      Total_P.ID=colSums(
        as.data.frame(cbind(
          Count=table(dataFull$P.ID),
          Percentage=round(prop.table(table(dataFull$P.ID))*100,1)))))
    )

Quotas$Census <- c(196, 263, 263, 288, 233, 258, 1500,
                   738, 762, 1500,
                   195, 180, 165,210,270,180, 180, 120, 1500,
                   210, 435, 435, 270, 150, 1500,
                   750,750, 1500)

Quotas$Difference <- Quotas$Count - Quotas$Census
Quotas$Percentage_Difference <- (Quotas$Count - Quotas$Census)*100/(Quotas$Count)
names(Quotas) <- c("Data Frequencies", "Proportion in %",
                   "U.S. Census Frequencies", "Data vs. Census",
                   "Data vs. Census in %")
#
#write.csv(Quotas, file = "Quotas PPBS 2016 by SSI.csv")
```

```{r Party ID Candidate and Party Preference Likeability}
#
##########################################################
# Party ID, Candidate and Party Preference, Likeability #
########################################################
#
# 1. PID - Dichotomous - Quotas 
# ------------------------------
# In terms of mainstream political parties, which do you feel closest to - or that best represents your views on political issues?
dataFull$Scr_PIDF <- factor(dataFull$Scr_PID, labels = c("Democrat", "Republican"))
#
dataFull$PID <- (dataFull$Scr_PID - 1)
dataFull$PIDF <- factor(dataFull$PID, 
                         labels = c("Democrat", "Republican"))
# identical(dataFull$Scr_PIDF, dataFull$PIDF)
#
#
#
#
# 2. Party ID 
# -------------------------------------------------------
# Generally speaking, do you usually think of yourself as a Republican, a Democrat, an Independent, or what?
# Would you call yourself a...  
# Strong Republican (4)  Not very strong Republican (3)
# Do you think of yourself as closer to...
# Republican Party (3)  Democrat Party (1)  No preference (2)
#
# Expanded to contain 11 options (not ordinal: hence nominal)
# -------------------------------
dataFull[dataFull$SC0_0 == "0", c("SC0_0")] <- NA
dataFull$SC0_0F <- factor(dataFull$SC0_0, 
                labels = c("Strong Democrat",
                           "Democrat",
                           "Leaning Democrat",
                           "Independent",
                           "Leaning Republican",
                           "Republican",
                           "Strong Republican",
                           "Don't Know",
                           "Leaning Democrat [DK]", 
                           "Leaning Republican  [DK]",
                           "Libertarian"))
#table(dataFull$SC0_0F)
dataFull$P.ID.11F <- dataFull$SC0_0F
#
# 
#
dataFull$PS_PID_ANES.F <- factor(dataFull$PS_PID_ANES,  labels = c("Democrat", "Independent", "Republican","Don't Know"))
#
# 2. Party ID - 9 points - ordinal
# -------------------------------------------------------
# Insert the Leaning DEM & GOP between ANES PID: recover 60 subjects
dataFull$SC0_0B <- dataFull$SC0_0
dataFull$SC0_0B[dataFull$SC0_0B == "10"] <- NA # DK
dataFull$SC0_0B[dataFull$SC0_0B == "13"] <- NA # Libertarian
#
dataFull$SC0_0B[dataFull$SC0_0B == "7"]  <- 9  
dataFull$SC0_0B[dataFull$SC0_0B == "6"]  <- 8
dataFull$SC0_0B[dataFull$SC0_0B == "5"]  <- 7
#  
dataFull$SC0_0B[dataFull$SC0_0B == "4"]  <- 5  
dataFull$SC0_0B[dataFull$SC0_0B == "11"] <- 4  # Leaning Democrat [DK]
dataFull$SC0_0B[dataFull$SC0_0B == "12"] <- 6  # Leaning Republican  [DK]
#
#table(dataFull$SC0_0B, useNA = "ifany")
#table(dataFull$SC0_0, useNA = "ifany")
# levels(dataFull$SC0_0F)
# table(dataFull$SC0_0F)
#
#
############################
# 2.1. Official ANES PID ##
##########################
dataFull$P.ID.ANES <- dataFull$SC0_0
dataFull$P.ID.ANES <- ifelse(dataFull$SC0_0 < 8, dataFull$P.ID.ANES, NA)
#table(dataFull$P.ID.ANES, useNA = "ifany")
dataFull$P.ID.ANES.F <- factor(dataFull$P.ID.ANES, 
                labels = c("Strong Democrat",
                           "Democrat",
                           "Leaning Democrat",
                           "Independent",
                           "Leaning Republican",
                           "Republican",
                           "Strong Republican"))
#table(dataFull$P.ID.ANES.F, useNA = "ifany")
#
# Creating Trichotomous PID with Leaaners as "independent"
dataFull$P.ID.ANES.F.w.Leanears.as.Ind <- recode(dataFull$P.ID.ANES.F,
                                             "c('Strong Democrat', 'Democrat')='Democrats'")
dataFull$P.ID.ANES.F.w.Leanears.as.Ind <- recode(dataFull$P.ID.ANES.F.w.Leanears.as.Ind,
                                             "c('Strong Republican', 'Republican')='Republicans'")
dataFull$P.ID.ANES.F.w.Leanears.as.Ind <- recode(dataFull$P.ID.ANES.F.w.Leanears.as.Ind,
                                             "c('Leaning Republican', 'Leaning Democrat', 'Independent')='Independent'")
#
#
# Creating Trichotomous PID with Leaaners as "independent"
dataFull$P.ID.ANES.F.w.Leanears <- recode(dataFull$P.ID.ANES.F,
                                             "c('Strong Democrat', 'Democrat', 'Leaning Democrat')='Democrats'")
dataFull$P.ID.ANES.F.w.Leanears <- recode(dataFull$P.ID.ANES.F.w.Leanears,
                                             "c('Strong Republican', 'Republican', 'Leaning Republican')='Republicans'")
#
#
#
#
#
# 3. Candidate Preference #
# -------------------------
# In the 2016 presidential election, which candidate do you feel the closest to - or best represents your views on political issues?
#dataFull$PS_Cand
dataFull$PS_CandF <- factor(dataFull$PS_Cand, 
                 labels = c("Ted Cruz", "Bernie Sanders", "Rand Paul", 
                            "Donald Trump", "Gary Johnson",
                            "Hillary Clinton",
                            "Jeb Bush", "Austin Petersen"))
# levels(dataFull$PS_CandF)
# table(dataFull$PS_CandF)
#
#
#
# 3. Candidate Preference #
# -------------------------
# 3.1 Democrat vs Republicans vs. Libertarians 
# ----------------------------------------------
#
dataFull$PS_Cand_PID <- dataFull$PS_Cand
library(car)
dataFull$PS_Cand_PID <- recode(dataFull$PS_Cand_PID,"c(1,3,4,7)='Republican'")
dataFull$PS_Cand_PID <- recode(dataFull$PS_Cand_PID,"c(2,6)='Democrats'")
dataFull$PS_Cand_PID <- recode(dataFull$PS_Cand_PID,"c(5,8)='Libertarians'")
#
#
# Coding Ideology and PID via candidates
dataFull$PS_Cand_PID_Ideo <- dataFull$PS_Cand
dataFull$PS_Cand_PID_Ideo <- recode(dataFull$PS_Cand_PID_Ideo, "c(1,3,4,7)='Conservative-Republicans'")
dataFull$PS_Cand_PID_Ideo <- recode(dataFull$PS_Cand_PID_Ideo, "c(2,6)='Liberal-Democrats'")
dataFull$PS_Cand_PID_Ideo <- recode(dataFull$PS_Cand_PID_Ideo, "c(5,8)='Libertarians'")
#
# Adding Rand Paul as Libertarian
dataFull$PS_Cand_PID_Ideo_v2 <- dataFull$PS_Cand
dataFull$PS_Cand_PID_Ideo_v2 <- recode(dataFull$PS_Cand_PID_Ideo_v2,"c(1,4,7)='Conservative-Republicans'")
dataFull$PS_Cand_PID_Ideo_v2 <- recode(dataFull$PS_Cand_PID_Ideo_v2,"c(2,6)='Liberal-Democrats'")
dataFull$PS_Cand_PID_Ideo_v2 <- recode(dataFull$PS_Cand_PID_Ideo_v2,"c(3,5,8)='Libertarians'")

#table(dataFull$PS_Cand_PID)
# revalue(junk$nm, c("B"="b"))
# subset(StudentdataFull, SchoolName=="Pine Tree Elementary" | Grade==3)
#
# 
#
# 3. Candidate Preference #
# -------------------------
# 3.2 Consistency  
# ---------------
# Given that the nominee of your preference did not succeed in winning the party nomination, who is the candidate that is the most likely to receive your vote?
# Donald Trump (1) 
# Gary Johnson (2)
# Hillary Clinton (3)
# None of these, Other (4)
# sum(!is.na(dataFull$PS_Cand_1)) #617 
#table(dataFull$PS_Cand_PID)
#table(dataFull$PS_Cand_1) # DT=157, GJ=51, HC=233, other=176
#table(dataFull$PS_Cand_1)
# Democrat 1 | Republican 2
#doBy::summaryBy(PS_Cand_1~Scr_PID, data=dataFull, FUN=c(table))
# unique(dataFull$PS_Cand_1)
dataFull$PS_Cand_Nominee <- factor(dataFull$PS_Cand_1, 
                        labels = c("Donald Trump", 
                                   "Gary Johnson",
                                   "Hilary Clinton", 
                                   "None/Other"))

#
# 4. Party Preference
# ---------------------
# Below you find a more comprehensive list in terms of the major political parties in the United States. Which one do you feel closest to - or that best represents your views on political issues?
#
dataFull$PS_PartiesF <- factor(dataFull$PS_Parties, 
        labels = c("Democratic Party",  "Republican Party", 
        "Libertarian Party", "Green Party", "Tea Party",
        "Constitution Party", "None", "Don't know"))
# levels(dataFull$PS_PartiesF)
# table(dataFull$PS_PartiesF)
#
#
# 5. Likeability
# ---------------
# Please elicit the extent to which you like or dislike each the main party's nominee for the 2016 elections
# 
# dataFull$PS_Like_1 #  Trump
# dataFull$PS_Like_2 #  Clinton
# dataFull$PS_Like_3 #  Johnson
#
dataFull$PS_Like_1_REV <- sapply(dataFull$PS_Like_1,  function(x) 10 - x)
dataFull$PS_Like_2_REV <- sapply(dataFull$PS_Like_2,  function(x) 10 - x)
dataFull$PS_Like_3_REV <- sapply(dataFull$PS_Like_3,  function(x) 10 - x)
#
#
#
# 6. Trumpism
# -------------
dataFull$PS_Cand_Trumpism <- dataFull$PS_Cand
library(car)
dataFull$PS_Cand_Trumpism <- car::recode(dataFull$PS_Cand,"c(1,3,7)='Republicans'")
dataFull$PS_Cand_Trumpism <- car::recode(dataFull$PS_Cand_Trumpism,"c(4)='Trumpists'")
dataFull$PS_Cand_Trumpism <- car::recode(dataFull$PS_Cand_Trumpism,"c(2,6)='Democrats'")
dataFull$PS_Cand_Trumpism <- car::recode(dataFull$PS_Cand_Trumpism,"c(5,8)='Libertarians'")
# table(dataFull$PS_Cand_Trumpism)
```

```{r Voting}
#######################################
# VOTE # Prospective # Retrospective #
#####################################
#
#
# 1. Are you a registered voter?
# ---------------------------
# table(dataFull$PS_Reg) # Yes: 1416, No: 84 
#
#
# 2. In the 2016 presidential election, will you vote?
# --------------------------------------------------
# table(dataFull$PS_Part) # Yes: 1375  No: 125 
#
#
#
# [Prospective] (if "Yes" Is Selected, then) 
# Who are you going to vote for?
# ------------------------------------------
dataFull$Vote_Pro <- factor(dataFull$PS_Vote_Pro, 
                        labels = c("Donald Trump", "Hilary Clinton", 
                                   "Gary Johnson", "Don't know"))
#
# table(dataFull$Vote_Pro, useNA = c("ifany")) # 1275 will vote, 
# Donald Trump Hilary Clinton   Gary Johnson     Don't know  <NA> 
#         546            557             81            191    125 
#
dataFull$Vote_16   <- dataFull$Vote_Pro
dataFull$Vote_16F  <- dataFull$Vote_Pro
#
#
# [Support] (Answer If "No" Is Selected) Given that you elicited that you will not vote, whose Presidency would you support?
# ------------------------------------------------------------------------------------
dataFull$Vote_Supp <- factor(dataFull$PS_Vote_Supp, 
                        labels = c("Donald Trump", "Hilary Clinton", 
                                   "Gary Johnson", "Don't know"))
#
# table(dataFull$Vote_Supp, useNA = c("ifany")) 
# Donald Trump Hilary Clinton Gary Johnson  Don't know   <NA> 
#         27            18           14           66     1375 
#
# [Retrospective 2012]
# ---------------------
#
# In the 2012 presidential election, who did you vote for? 
dataFull$Vote_12  <- factor(dataFull$PS_Vote_Retro12 , 
                        labels = c("Mitt Romney", "Barak Obama",
                                   "Don't know/Didn't vote"))
#
# table(dataFull$Vote_12)
# Mitt Romney            Barak Obama    Don't know/Didn't vote 
#       576                    660                        264 
#
#
# [Retrospective 2008]
# ---------------------
#
# In the 2008 presidential election, who did you vote for? 
dataFull$Vote_08 <- factor(dataFull$PS_Vote_Retro08, 
                        labels = c("John McCain", "Barak Obama",
                                   "Don't know/Didn't vote"))
# table(dataFull$Vote_08)
#          John McCain        Barak Obama    Don't know/Didn't vote 
#               555                    651                    294 
#
#
#
#
#
# [Prospective & Support] (if "Yes" Is Selected for either) 
# 
#
dataFull$Vote_16.PROnSUPP  <- rowSums(cbind(
  dataFull$PS_Vote_Pro, dataFull$PS_Vote_Supp),
  na.rm = T)
#
#table(dataFull$Vote_16.PROnSUPP)
dataFull$Vote_16.PROnSUPP[dataFull$Vote_16.PROnSUPP == "0"] <- NA # this is necessary because when you sum NAs it gives 0 > rowSums(cbind(c(NA),c(NA)), na.rm = T)
#
dataFull$Vote_16.PROnSUPP.D <- dataFull$Vote_16.PROnSUPP
dataFull$Vote_16.PROnSUPP.D[dataFull$Vote_16.PROnSUPP.D == "3"] <- NA # 
dataFull$Vote_16.PROnSUPP.D[dataFull$Vote_16.PROnSUPP.D == "4"] <- NA # 
#
#
dataFull$Vote_16.PROnSUPP.D <- (dataFull$Vote_16.PROnSUPP.D - 1)
#
dataFull$Vote_16.PROnSUPP.DF <- factor(dataFull$Vote_16.PROnSUPP.D,
                         labels = c("Donald Trump", "Hilary Clinton"))
#
#table(dataFull$Vote_16.PROnSUPP,  useNA = "ifany")
#table(dataFull$Vote_16.PROnSUPP.D,  useNA = "ifany")
#

#
#
#
# Vote 2016 - Dichotomous - Is registered, will vote
# ----------------------------------------------------
dataFull$Vote_16D  <- dataFull$Vote_16
dataFull$Vote_16D[dataFull$Vote_16D == "Gary Johnson"] <- NA 
dataFull$Vote_16D[dataFull$Vote_16D == "Don't know"]   <- NA 
#dataFull$Vote_16D.N <- (2-as.numeric(dataFull$Vote_16D))
#
dataFull$Vote_12D  <- dataFull$Vote_12
dataFull$Vote_12D[dataFull$Vote_12D == "Don't know/Didn't vote"] <- NA
#
dataFull$Vote_08D  <- dataFull$Vote_08
dataFull$Vote_08D[dataFull$Vote_08D == "Don't know/Didn't vote"] <- NA
```

```{r Volatility}
###############
# Volatility #
#############
#
# 0.[?] Given that the nominee of your preference did not succeed in winning the party nomination, who is the candidate that is the most likely to receive your vote?
# ----------------------------------------------------------
#table(dataFull$PS_Vol_PID_1)
#doBy::summaryBy(PS_Cand_1~Scr_PID, data=dataFull, FUN=c(table))
# 
#
#
# 1.[Answer If "None of these, Other"" Is Selected above] 
# I normally self-identify with only one party. But in this 2016 Presidential election, the candidate chosen by my party does not represent me. For and for this reason I am considering...
# ----------------------------------------------------------
# Voting for the other main party (1)
# Voting for another - not mainstream - party (2)
# Casting a blank ballot (3)
# Not voting (4)
# This statement does not apply to me (5)
# table(dataFull$PS_Vol_PID_1)
# 1  2  3  4  5 
# 7 66 21 68 14
#
#
#
# [Volatility] 2008 - 2012
# -------------------------
#
library(mosaic)
myfile <- plyr::mutate(dataFull, Volatility = derivedFactor(
    "FirmRepublican"   = (PS_Vote_Retro12==1 & PS_Vote_Retro08==1),
    "FirmDemocrat"     = (PS_Vote_Retro12==2 & PS_Vote_Retro08==2),
    "Dem-to-Rep"       = (PS_Vote_Retro12==2 & PS_Vote_Retro08==1),
    "Rep-to-Dem"       = (PS_Vote_Retro12==1 & PS_Vote_Retro08==2),
    "DK-DR"            = (PS_Vote_Retro12==3 & PS_Vote_Retro08==3),
    .method = "unique", .default = "DK"))
dataFull$Volatility <- myfile$Volatility
#table(dataFull$Volatility)
