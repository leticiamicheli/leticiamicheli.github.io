#             Vote_16F = data.76$Vote_16F)

#View(doBy::summaryBy(cbind(SELF, OTHER) ~ PS_CandF + Vote_16F, FUN=mean, data=df2))
#View(doBy::summaryBy(cbind(SELF, OTHER) ~ cbind(PS_CandF), FUN=mean, data=df2))
