```{r Libertarianism}
###################
# Libertarianism #
#################
#
# In terms of your political views, do you usually think of yourself as a Libertarian?
dataFull$Libertarian <- factor(dataFull$PS_Lib_PID , 
                        labels = c("Libertarian", "Non-Libertarian"))
#table(dataFull$Libertarian)
#
#
#
#
# The Libertarian Party is considered to be America's third-party. What is the degree to which it represents your political views.Completely represents my political views(left=1) Does not represents my political views at all (right=9)
dataFull$Lib_Pol_Views <- sapply(dataFull$PS_Lib_Rep,  function(x) 10 - x)
# plot(table(dataFull$PS_Lib_Rep))
#
#
###########
# Scale ##
##########
#
# Facets: State Control, Economic, Social/Tradition and ?State?
# --------------------------------------------------------------
#
# [State Control] Military service ought to be voluntary
# [Soc-T] There should not be laws regulating the sexual activity between consenting adults
# [Soc-T] There should be no laws prohibiting adult possession and use of drugs
# [State Control] There should not be a National Identity card
# [Eco] No welfare and no restrictions on work
# [Soc-T] De-regulate drugs, alcohol, prostitution, gambling, and suicide
# [Eco] Eliminate the minimum wage in every state
# [State Control] Repeal all gun control laws and affirm the right to keep and bear arms
# [State Control] The government interferes far too much in our everyday lives
# [Soc-T] People should be free to decide which group-norms or traditions to follow
# [State] The government is practically always wasteful and inefficient
# [Eco] We should reduce taxes, spending, and eliminate controls on trade
# [REV] [Soc-T] The government main goal is to improve the common good, even if that limits the some of the civil liberties of individuals
# [State Control] Repeal Obama Care and don't replace it with RomneyCare
# [Soc-T] Government should be kept out of the matter of abortion
# [State Control] It is OK for companies to deny service to anyone (gays or otherwise) as well as it is OK to boycott those companies
# [Eco] Abolish all regulation of banks in favor of free market
# [Eco] Free-market banking: unrestricted competition and no bailouts
# [Eco] Pass constitutional amendment requiring a balanced budget
# Oppose gerrymandering and restrictions on ballot access
# [Eco] Phase out government-sponsored retirement system
# [State Control] Repeal the Patriot Act and end state surveillance
#
#
dataFull$Lib_1_REV  <- sapply(dataFull$Lib_1,  function(x) 10 - x)
dataFull$Lib_2_REV  <- sapply(dataFull$Lib_2,  function(x) 10 - x)
dataFull$Lib_3_REV  <- sapply(dataFull$Lib_3,  function(x) 10 - x)
dataFull$Lib_4_REV  <- sapply(dataFull$Lib_4,  function(x) 10 - x)
dataFull$Lib_5_REV  <- sapply(dataFull$Lib_5,  function(x) 10 - x)
dataFull$Lib_6_REV  <- sapply(dataFull$Lib_6,  function(x) 10 - x)
dataFull$Lib_7_REV  <- sapply(dataFull$Lib_7,  function(x) 10 - x)
dataFull$Lib_8_REV  <- sapply(dataFull$Lib_8,  function(x) 10 - x)
dataFull$Lib_9_REV  <- sapply(dataFull$Lib_9,  function(x) 10 - x)
dataFull$Lib_10_REV <- sapply(dataFull$Lib_10,  function(x) 10 - x)
dataFull$Lib_11_REV <- sapply(dataFull$Lib_11,  function(x) 10 - x)
dataFull$Lib_12_REV <- sapply(dataFull$Lib_12,  function(x) 10 - x)
# 13 not
dataFull$Lib_14_REV <- sapply(dataFull$Lib_14,  function(x) 10 - x)
dataFull$Lib_15_REV <- sapply(dataFull$Lib_15,  function(x) 10 - x)
dataFull$Lib_16_REV <- sapply(dataFull$Lib_16,  function(x) 10 - x)
dataFull$Lib_17_REV <- sapply(dataFull$Lib_17,  function(x) 10 - x)
dataFull$Lib_18_REV <- sapply(dataFull$Lib_18,  function(x) 10 - x)
dataFull$Lib_19_REV <- sapply(dataFull$Lib_19,  function(x) 10 - x)
dataFull$Lib_20_REV <- sapply(dataFull$Lib_20,  function(x) 10 - x)
dataFull$Lib_21_REV <- sapply(dataFull$Lib_21,  function(x) 10 - x)
dataFull$Lib_22_REV <- sapply(dataFull$Lib_22,  function(x) 10 - x)
#
dataFull$Lib   <- rowMeans(dataFull[,c(
  "Lib_1_REV", "Lib_2_REV", "Lib_3_REV", "Lib_4_REV", "Lib_5_REV",
  "Lib_6_REV", "Lib_7_REV", "Lib_8_REV", "Lib_9_REV", "Lib_10_REV",
  "Lib_11_REV", "Lib_12_REV", "Lib_13", "Lib_14_REV", "Lib_15_REV",
  "Lib_16_REV", "Lib_17_REV", "Lib_18_REV", "Lib_19_REV", "Lib_20_REV",
  "Lib_21_REV", "Lib_22_REV")], na.rm  =TRUE)
#
#
# summary(dataFull$Lib)
