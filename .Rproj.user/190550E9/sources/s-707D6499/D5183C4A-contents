```{r Demographics - Screening}
################
# Demographics #
################
# Screening #
#############
#
# 1. What is your age?
# ---------------------
#18 to 24 years (1)
#25 to 34 years (2)
#35 to 44 years (3)
#45 to 54 years (4)
#55 to 64 years (5)
#65+ (6)
#
#
#table(dataFull$Scr_Age)
#
dataFull$Age  <- dataFull$Scr_Age
dataFull$AgeF <- factor(dataFull$Scr_Age,
                    labels = c("18-24 years", "25-34 years", 
                               "35-44 years","45-54 ycccccccears",
                               "55-64 years", "65+"))
# as.data.frame(cbind(Count=table(dataFull$AgeF),Percentage=round(prop.table(table(dataFull$AgeF))*100,1)))
#
#
#
# 2. What is your education level?
# ----------------------------------
#Less than high school (1)
#High school graduate (includes equivalency) (2)
#Some college, no degree (3)
#Bachelor's degree (4)
#Graduate or professional degree (5)
# 
#
#
# table(dataFull$Scr_Edu)
dataFull$Edu  <- dataFull$Scr_Edu
dataFull$EduF <- factor(dataFull$Scr_Edu,
                    labels = c("Less than High-school", 
                               "High-school","Some college",
                               "Bachelor",  "Graduate"))
# as.data.frame(cbind(Count=table(dataFull$EduF),Percentage=round(prop.table(table(dataFull$EduF))*100,1)))
#
#
#
#
#
# 3. What is your current gross annual income (before taxes)?
# -----------------------------------------------------------
#Less than $15,000 (1)
#$15,000 to $24,999 (2)
#$25,000 to $34,999 (3)
#$35,000 to $49,999 (4)
#$50,000 to $74,999 (5)
#$75,000 to $99,999 (6)
#$100,000 to $149,999 (7)
#$150,000 more (8)
# 
#
#
# table(dataFull$Scr_Inc)
#
dataFull$Inc  <- dataFull$Scr_Inc
dataFull$IncF <- factor(dataFull$Scr_Inc,
              labels = c("Less than $15,000", "$15,000-$24,999",
                         "$25,000-$34,999"  , "$35,000-$49,999", 
                         "$50,000-$74,999"  , "$75,000-$99,999",
                         "$100,000-$149,999", "$150,000 +"))
# as.data.frame(cbind(Count=table(dataFull$IncF),Percentage=round(prop.table(table(dataFull$IncF))*100,1)))
#
#
#
# 
# 4. # What is your gender?
# -------------------------
#Female (1)
#Male (2)
# 
#
# table(dataFull$Scr_Sex)
dataFull$Sex  <- dataFull$Scr_Sex
dataFull$SexF <- factor(dataFull$Scr_Sex, labels = c("Female", "Male"))
# as.data.frame(cbind(Count=table(dataFull$SexF),Percentage=round(prop.table(table(dataFull$SexF))*100,1)))
#
#
#
#
# 
# 5. In terms of mainstream political parties, which do you feel closest to - or that best represents your views on political issues?
# ------------------------------------------------------------------
#Democrat (1)
#Republican (2)
#
#
# table(dataFull$Scr_PID)
#
dataFull$P.ID <- factor(dataFull$Scr_PID, 
                    labels = c("Democrat", "Republican"))
# as.data.frame(cbind(Count=table(dataFull$P.ID),Percentage=round(prop.table(table(dataFull$P.ID))*100,1)))
```

```{r Demographics - Block}
################
# Demographics #
################
# Block #
#########
#
# 1. What is your ethnicity?
# --------------------------
#Caucasian/European origin (1)
#Black/African American (2)
#Latino (3)
#Asian/Pacific Islander (4)
#Native American (5)
#Other (6) 
# 
#
# table(dataFull$Dem_Eth)
#
dataFull$Eth  <- dataFull$Dem_Eth
dataFull$EthF <- factor(dataFull$Dem_Eth,
              labels = c("Caucasian/European origin",
                         "Black/African American",
                         "Latino", 
                         "Asian/Pacific Islander", 
                         "Native American",
                         "Other"))


# 2. What is your occupation?
# ---------------------------
#Student (1)
#Employed (2)
#Unemployed (3)
#Full-time caregiver (4)
#Retired (5)
#Parent (6)
#Disabled (7)
# 
#
# table(dataFull$Dem_Job)
#
dataFull$Occ  <- dataFull$Dem_Job
dataFull$OccF <- factor(dataFull$Dem_Job,
              labels = c("Student",
                         "Employed",
                         "Unemployed", 
                         "Full-time caregiver", 
                         "Retired",
                         "Parent",
                         "Disabled"))
#
#
#
#
#
# 3. What is your religious affiliation?
# --------------------------------------
# Christian (1)
# Muslim (2)
# Jewish (3)
# Atheist/Agnostic (4)
# Not listed (5)
# 
#
# table(dataFull$Dem_Rel)
#
dataFull$Rel  <- dataFull$Dem_Rel
dataFull$RelF <- factor(dataFull$Dem_Rel,
              labels = c("Christian",
                         "Muslim",
                         "Jewish", 
                         "Atheist/Agnostic", 
                         "Not listed, No religion"))





# 4. What type of area do you currently live in?
# ----------------------------------------------
# Urban (1)
# Rural (2)
#
#
#
# table(dataFull$Dem_Area)
#
dataFull$Area  <- dataFull$Dem_Area
dataFull$AreaF <- factor(dataFull$Dem_Area,
              labels = c("Urban", "Rural"))
#
#
#
#
#
# 5. In which state do you currently live?
# ----------------------------------------------
# 
# 
#
#table(dataFull$Dem_State)
