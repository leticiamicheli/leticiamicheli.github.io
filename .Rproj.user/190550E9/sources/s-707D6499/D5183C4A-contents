dataFull$PVQ_44 <- rowSums(cbind(dataFull$PVQ_RR_Male_44, dataFull$PVQ_RR_Fem_44), na.rm=TRUE)
dataFull$PVQ_45 <- rowSums(cbind(dataFull$PVQ_RR_Male_45, dataFull$PVQ_RR_Fem_45), na.rm=TRUE)
dataFull$PVQ_46 <- rowSums(cbind(dataFull$PVQ_RR_Male_46, dataFull$PVQ_RR_Fem_46), na.rm=TRUE)
dataFull$PVQ_47 <- rowSums(cbind(dataFull$PVQ_RR_Male_47, dataFull$PVQ_RR_Fem_47), na.rm=TRUE)
dataFull$PVQ_48 <- rowSums(cbind(dataFull$PVQ_RR_Male_48, dataFull$PVQ_RR_Fem_48), na.rm=TRUE)
dataFull$PVQ_49 <- rowSums(cbind(dataFull$PVQ_RR_Male_49, dataFull$PVQ_RR_Fem_49), na.rm=TRUE)
dataFull$PVQ_50 <- rowSums(cbind(dataFull$PVQ_RR_Male_50, dataFull$PVQ_RR_Fem_50), na.rm=TRUE)
dataFull$PVQ_51 <- rowSums(cbind(dataFull$PVQ_RR_Male_51, dataFull$PVQ_RR_Fem_51), na.rm=TRUE)
dataFull$PVQ_52 <- rowSums(cbind(dataFull$PVQ_RR_Male_52, dataFull$PVQ_RR_Fem_52), na.rm=TRUE)
dataFull$PVQ_53 <- rowSums(cbind(dataFull$PVQ_RR_Male_53, dataFull$PVQ_RR_Fem_53), na.rm=TRUE)
dataFull$PVQ_54 <- rowSums(cbind(dataFull$PVQ_RR_Male_54, dataFull$PVQ_RR_Fem_54), na.rm=TRUE)
dataFull$PVQ_55 <- rowSums(cbind(dataFull$PVQ_RR_Male_55, dataFull$PVQ_RR_Fem_55), na.rm=TRUE)
dataFull$PVQ_56 <- rowSums(cbind(dataFull$PVQ_RR_Male_56, dataFull$PVQ_RR_Fem_56), na.rm=TRUE)
dataFull$PVQ_57 <- rowSums(cbind(dataFull$PVQ_RR_Male_57, dataFull$PVQ_RR_Fem_57), na.rm=TRUE)
