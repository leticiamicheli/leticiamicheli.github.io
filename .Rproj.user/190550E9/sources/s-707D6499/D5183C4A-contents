dataFull$Pop_3_REV  <- sapply(dataFull$Pop_3,  function(x) 10 - x)
dataFull$Pop_4_REV  <- sapply(dataFull$Pop_4,  function(x) 10 - x)
dataFull$Pop_7_REV  <- sapply(dataFull$Pop_7,  function(x) 10 - x)
#
#
dataFull$Pop_Anti_Elitism  <- rowMeans(dataFull[,c("Pop_2_REV", "Pop_3_REV", "Pop_4_REV", "Pop_7_REV")], na.rm=TRUE)
#
# 4. Facet: Representative Gap [8, 9, 10]
# ----------------------------------------------
# Public officials don't care much about what people like me think
# Elected officials in Washington lost touch with the people's needs
# The system is stacked against people like me
#dataFull$Pop_Gap  <- sapply(dataFull$Pop_8,  function(x) 10 - x) +
 # sapply(dataFull$Pop_9,  function(x) 10 - x) +
  #sapply(dataFull$Pop_10,  function(x) 10 - x)
#
#
dataFull$Pop_8_REV  <- sapply(dataFull$Pop_8,  function(x) 10 - x)
