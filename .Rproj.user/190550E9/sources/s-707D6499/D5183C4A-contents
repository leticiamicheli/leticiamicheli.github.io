### Creating "Working Data.RData"

```{r Creating datasets}
1
#load("C:/Users/falaf/Dropbox/Shared/Projects/Current/Tobias Rothmund/_OriginalData/1st Study/Working Data Full.RData")
#
load("Working Data Full.RData")
#
labels <- readRDS("PPBS 2016 [labels].rds")
#
# Separating into different datasets
# ------------------------------------
#
dataQF <- dataFull[dataFull$Index == "QF",]
dataOut <- dataFull[dataFull$Index == "Out",]
dataIncomplete <- dataFull[dataFull$Index == "Incomplete",]
data.76 <- dataFull[dataFull$Index == "Data1576",]
#
#
#
dataQF <- sjlabelled::copy_labels(df_new = dataQF, df_origin = labels)
dataOut <- sjlabelled::copy_labels(df_new = dataOut, df_origin = labels)
dataIncomplete <- sjlabelled::copy_labels(df_new = dataIncomplete, df_origin = labels)
data.76 <- sjlabelled::copy_labels(df_new = data.76, df_origin = labels)
#
#
#############
# data1576 #
###########
# Qualtrics estimated completion time was 65.1 minutes
# We used 1/3 of that time as threshold, 65.1/3 ~ 21.7
data.76.Out <- data.76[(data.76$Q_TotalDuration/60) <= 21.7, ]
data.76 <- data.76[(data.76$Q_TotalDuration/60) > 21.7, ]
#
# Now check for AQ
#sum(ifelse(data.76$Attention.Score > 1, 1, 0))
#
# Because there is data from the Pre-launch, we check for SC1_0 as well
# We find 4 AQ larger than 1. These need to be excluded.
data.76$V8     <- as.Date(data.76$V8, format= "%m/%d/%Y")
#sum(ifelse(data.76$SC1_0 > 1, 1, 0))
data.76.Out.2 <- data.76[data.76$SC1_0 > 1, ]
data.76 <- data.76[data.76$SC1_0 < 2, ]
data.76 <- sjlabelled::copy_labels(df_new = data.76, df_origin = labels)
#
#
###########
# dataQF #
#########
#
# Total duration larger than ~22 min. 
dataQF.Out <- dataQF[(dataQF$Q_TotalDuration/60) <= 21.7, ]
dataQF <- dataQF[(dataQF$Q_TotalDuration/60) > 21.7, ]
dataQF <- sjlabelled::copy_labels(df_new = dataQF, df_origin = labels)

#
#
# Now check for AQ
#sum(ifelse(dataQF$Attention.Score > 1, 1, 0))
#sum(ifelse(dataQF$SC1_0 > 1, 1, 0))
# All good!
# QF has 2119 cases then.
#
#
############
# dataOut #
##########
#
# Here we have to include the 'excluded' data from the above datasets
# Step by step
data.76.Out <- rbind(data.76.Out, data.76.Out.2)
data.Out    <- rbind(data.76.Out, dataQF.Out)
dataOut     <- rbind(dataOut, data.Out)
#
#
#c("data", "data.76", "data.76.Out", "data.76.Out.2", "data.Out", "dataFull", "dataIncomplete", "dataOut", "dataQF", "dataQF.Out", "labels")
#
nrow(data.76) +           # 1500  [NR]
  nrow(data.76.Out) +     # 76
  nrow(data.76.Out.2) +   # 4
  nrow(dataQF) +          # 2119  [CF]
  nrow(data.Out) +        # 80
  nrow(dataOut) +         # 928
  nrow(dataIncomplete) +  # 1302
  nrow(dataQF.Out)        # 4
#
# 6013 - 5849 # 164
# 76 + 4 + 80 + 4 = 164 - these are additional. This is in dataOut.
# 1500 + 2119 + 1302 + 928 = 5849 
#
# Now remove un-necessary datasets, that are in dataOut
#
rm(list = c('data.76.Out','data.76.Out.2', 'dataQF.Out', 'data.Out'))
#
#
###################
# Processed data #
#################
#
# Now aggregate separate datasets into one dataset.
data <- rbind(data.76, dataQF, dataOut, dataIncomplete)
data <- sjlabelled::copy_labels(df_new = data, df_origin = labels)
# Which has same number of cases ans dataFull
#
# Now save image (Rdata file) and .csv files
# change working directory to your
#
#
rm(list = c('labels', 'data'))
#
save.image(file="Working Data.RData")
#
```

```{r Quotas for Nationally Representative data}
#
#######################
# Quotas - Screening #
#####################
Quotas <- 
  rbind(
    rbind(
      as.data.frame(cbind(
        Count=table(data.76$AgeF),
        Percentage=round(prop.table(table(data.76$AgeF))*100,1))),
        Total_Age=colSums(
          as.data.frame(cbind(
            Count=table(data.76$AgeF),
            Percentage=round(prop.table(table(data.76$AgeF))*100,1))))),
    
    rbind(as.data.frame(cbind(
      Count=table(data.76$SexF),
      Percentage=round(prop.table(table(data.76$SexF))*100,1))),
      Total_Gender=colSums(
        as.data.frame(cbind(
          Count=table(data.76$SexF),
          Percentage=round(prop.table(table(data.76$SexF))*100,1))))),
    
    rbind(as.data.frame(cbind(
      Count=table(data.76$IncF),
      Percentage=round(prop.table(table(data.76$IncF))*100,1))),
      Total_Income=colSums(
        as.data.frame(cbind(
          Count=table(data.76$IncF),
          Percentage=round(prop.table(table(data.76$IncF))*100,1))))),
  
    rbind(as.data.frame(cbind(
      Count=table(data.76$EduF),
      Percentage=round(prop.table(table(data.76$EduF))*100,1))),
      Total_Education=colSums(
        as.data.frame(cbind(
          Count=table(data.76$EduF),
          Percentage=round(prop.table(table(data.76$EduF))*100,0))))),
  
    rbind(as.data.frame(cbind(
      Count=table(data.76$P.ID),
      Percentage=round(prop.table(table(data.76$P.ID))*100,1))),
      Total_P.ID=colSums(
        as.data.frame(cbind(
          Count=table(data.76$P.ID),
          Percentage=round(prop.table(table(data.76$P.ID))*100,1)))))
    )

Quotas$Census <- c(196, 263, 263, 288, 233, 258, 1500,
                   738, 762, 1500,
                   195, 180, 165,210,270,180, 180, 120, 1500,
                   210, 435, 435, 270, 150, 1500,
                   750,750, 1500)

Quotas$Difference <- Quotas$Count - Quotas$Census
Quotas$Percentage_Difference <- (Quotas$Count - Quotas$Census)*100/(Quotas$Count)
names(Quotas) <- c("Data Frequencies", "Proportion in %",
                   "U.S. Census Frequencies", "Data vs. Census",
                   "Data vs. Census in %")
#
write.csv(Quotas, file = "SSI quota - w out revision [original - as in Womick].csv")
rm("data.76", "dataFull", "dataIncomplete", "dataOut", "dataQF")
