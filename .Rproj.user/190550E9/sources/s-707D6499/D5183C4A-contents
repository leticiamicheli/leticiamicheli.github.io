NR.PPBS2016 <- NR.PPBS2016.raw[, names(NR.PPBS2016.raw) %in% c(variables.to.filter.in)]
#
CF.PPBS2016 <- CF.PPBS2016.raw[, names(CF.PPBS2016.raw) %in% c(variables.to.filter.in)]
#
NRCF.PPBS2016 <- NRCF.PPBS2016.raw[, names(NRCF.PPBS2016.raw) %in% c(variables.to.filter.in)]
#
#
NR.PPBS2016   <- NR.PPBS2016[NR.PPBS2016$respodent_status %in% c("Complete"),]
CF.PPBS2016   <- CF.PPBS2016[CF.PPBS2016$respodent_status %in% c("Complete"),]
NRCF.PPBS2016 <- NRCF.PPBS2016[NRCF.PPBS2016$respodent_status %in% c("Complete"),]
#
labels <- readRDS("PPBS 2016 [labels].rds")
#
NR.PPBS2016 <- sjlabelled::copy_labels(df_new = NR.PPBS2016,
                                       df_origin = labels)
#
CF.PPBS2016 <- sjlabelled::copy_labels(df_new = CF.PPBS2016,
                                       df_origin = labels)
#
NRCF.PPBS2016 <- sjlabelled::copy_labels(df_new = NRCF.PPBS2016,
                                         df_origin = labels)
#
#
#
#################################
# Saving data ##################
###############################
#
haven::write_sav(NR.PPBS2016, "_data_processed/PPBS US 2016 NR.sav")
haven::write_sav(CF.PPBS2016, "_data_processed/PPBS US 2016 CF.sav")
#
write.csv(NR.PPBS2016, "_data_processed/PPBS US 2016 NR.csv")
write.csv(CF.PPBS2016, "_data_processed/PPBS US 2016 CF.csv")
#
save(NR.PPBS2016, CF.PPBS2016, NRCF.PPBS2016, 
     file = "_data_processed/PPBS US 2016 NR & CF.RData")
#
#
rm("CF.PPBS2016", "NR.PPBS2016",  "NRCF.PPBS2016", "variables.to.filter.in", "ResponseID.NR.Incomplete", "ResponseID.NR.Qual.Fail")
#
```

## Creating Working data

```{r}
NR.PPBS2016.WD   <- NR.PPBS2016.raw[NR.PPBS2016.raw$respodent_status %in% c("Complete"),]
CF.PPBS2016.WD   <- CF.PPBS2016.raw[CF.PPBS2016.raw$respodent_status %in% c("Complete"),]
NRCF.PPBS2016.WD <- NRCF.PPBS2016.raw[NRCF.PPBS2016.raw$respodent_status %in% c("Complete"),]
#
#
#
labels <- readRDS("PPBS 2016 [labels].rds")
#
NR.PPBS2016.WD <- sjlabelled::copy_labels(df_new = NR.PPBS2016.WD,
                                          df_origin = labels)
#
CF.PPBS2016.WD <- sjlabelled::copy_labels(df_new = CF.PPBS2016.WD,
                                          df_origin = labels)
#
NRCF.PPBS2016.WD <- sjlabelled::copy_labels(df_new = NRCF.PPBS2016.WD,
                                            df_origin = labels)
#
save(NR.PPBS2016.WD, CF.PPBS2016.WD, NRCF.PPBS2016.WD, 
     file = "Working Data PPBS2016.RData")
#
#
rm("NR.PPBS2016.WD", "CF.PPBS2016.WD",  "NRCF.PPBS2016.WD")
```

## Creating Raw data

**For methods.Rmd**

```{r}
save(NR.PPBS2016.raw, CF.PPBS2016.raw, NRCF.PPBS2016.raw, 
     file = "PPBS2016.raw.RData")
#
#
rm("NR.PPBS2016.raw", "CF.PPBS2016.raw",  "NRCF.PPBS2016.raw", "ResponseID.NR.Incomplete", "ResponseID.NR.Qual.Fail", "%!in%")
```

