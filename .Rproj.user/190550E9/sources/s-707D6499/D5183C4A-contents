#dataFull$CS1_ZB <- dataFull$CS1_ZB_1_REV + dataFull$CS1_ZB_2_REV + dataFull$CS1_ZB_3 + dataFull$CS1_ZB_4 + dataFull$CS1_ZB_5_REV + dataFull$CS1_ZB_6_REV + dataFull$CS1_ZB_7_REV + dataFull$CS1_ZB_8_REV + dataFull$CS1_ZB_9 + dataFull$CS1_ZB_10 + dataFull$CS1_ZB_11_REV + dataFull$CS1_ZB_12

dataFull$CS1_ZB <- rowMeans(dataFull[,c("CS1_ZB_1", "CS1_ZB_2", "CS1_ZB_3_REV", "CS1_ZB_4_REV", "CS1_ZB_5", "CS1_ZB_6", "CS1_ZB_7", "CS1_ZB_8", "CS1_ZB_9_REV", "CS1_ZB_10_REV", "CS1_ZB_11", "CS1_ZB_12_REV")], na.rm=TRUE)
